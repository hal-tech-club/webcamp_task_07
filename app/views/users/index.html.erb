<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>
        image
      </th>
      <th>
        name
      </th>
      <th>
      </th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%= user.profile_image_id %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "Show", user_path(user.id) %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
